	targetNamespace="http://schemas.microsoft.com/wix/2006/wi"
	xmlns="http://schemas.microsoft.com/wix/2006/wi">

	<xs:element name="Property">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
